{
    "name": "visual-flow-ui",
    "version": "1.0.0",
    "private": true,
    "license": "Apache-2.0",
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@material-ui/styles": "^4.10.0",
        "axios": "^0.21.1",
        "classnames": "^2.2.6",
        "dotenv": "^8.2.0",
        "history": "^4.10.1",
        "i18next": "^19.7.0",
        "jest-sonar-reporter": "^2.0.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "mxgraph": "^4.2.0",
        "nanoid": "^3.1.12",
        "notistack": "^1.0.0",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-i18next": "^11.7.2",
        "react-redux": "^7.1.1",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react-router-redux": "^4.0.8",
        "redux": "^4.0.4",
        "redux-logger": "^3.0.6",
        "redux-promise": "^0.6.0",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/node": "^7.6.3",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/plugin-proposal-optional-chaining": "^7.8.3",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.11.5",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@types/jest": "^27.0.2",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.4.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "eslint": "^7.8.1",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^2.4.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^2.3.0",
        "fast-sass-loader": "^1.5.0",
        "file-loader": "^6.2.0",
        "html-loader": "^2.1.2",
        "jest": "^27.3.1",
        "jest-junit": "^13.0.0",
        "loader-utils": "^2.0.0",
        "mini-css-extract-plugin": "^2.4.2",
        "node-sass": "^6.0.1",
        "owasp-dependency-check": "^0.0.18",
        "prettier": "^1.19.1",
        "react-test-renderer": "^16.12.0",
        "redux-mock-store": "^1.5.4",
        "style-loader": "^3.3.0",
        "webpack": "~5.58.1",
        "webpack-cli": "^4.9.0",
        "webpack-node-externals": "^3.0.0"
    },
    "scripts": {
        "start": "webpack --config webpack.dev.js --watch",
        "build": "webpack --config webpack.config.js --mode production",
        "owasp": "owasp-dependency-check --project frontend --scan package-lock.json --scan package.json -f HTML --disableAssembly --failOnCVSS 8 --suppression dependency-check-suppression.xml",
        "test": "jest && npm run owasp",
        "coverage": "jest --coverage",
        "format": "prettier --write \"./src/**/*.{js,md,scss}\"",
        "format:diff": "prettier --list-different \"./src/**/*.{js,md,scss}\"",
        "lint": "eslint ./src -f json -o reports/eslint-report.json",
        "lint-fix": "eslint --fix ./src"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.js"
        ],
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
        "reporters": [
            "default",
            "jest-junit"
        ],
        "coverageReporters": [
            "text",
            "lcov",
            "cobertura"
        ],
        "collectCoverageFrom": [
            "src/**/*.js",
            "!src/**/*.test.js",
            "!src/__tests__/**",
            "!src/__mocks__/**",
            "!**/setupTests.js"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
            "\\.(scss|css|less)$": "<rootDir>/src/__mocks__/styleMock.js"
        },
        "testResultsProcessor": "jest-sonar-reporter",
        "testEnvironment": "jsdom"
    },
    "jest-junit": {
        "outputDirectory": "coverage/junit",
        "outputName": "junit.xml",
        "usePathForSuiteName": "true"
    },
    "jestSonar": {
        "reportPath": "reports",
        "reportFile": "test-report.xml"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
